package com.blog.payloads;

import jakarta.validation.constraints.NotEmpty;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;
@Getter
@Setter
@NoArgsConstructor
public class CategoriDto {

	private Integer categoriId;
	@NotEmpty(message= "please provide categori title.")
	private String categoriTitle;
	@NotEmpty(message= "Please provide categori Description.")
	private String categoriDescription;

}
